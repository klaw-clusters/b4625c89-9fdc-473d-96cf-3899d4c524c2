{
    "kind": "Service",
    "apiVersion": "serving.knative.dev/v1",
    "metadata": {
        "name": "default-sd-angela-test-outlier-find me an outlier",
        "namespace": "default",
        "creationTimestamp": null,
        "labels": {
            "seldon.io/deployment-kind": "SeldonDeployment",
            "seldon.io/deployment-name": "angela-test",
            "seldon.io/deployment-namespace": "default",
            "seldon.io/detector-name": "find me an outlier",
            "seldon.io/detector-type": "outlier"
        }
    },
    "spec": {
        "template": {
            "metadata": {
                "creationTimestamp": null,
                "labels": {
                    "fluentd": "true",
                    "seldon-deployment-id": "angela-test",
                    "seldon.io/outlier": "true"
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "outlier-detector",
                        "image": "seldonio/alibi-detect-server:1.11.2",
                        "args": [
                            "--model_name",
                            "find me an outlier",
                            "--http_port",
                            "8080",
                            "--protocol",
                            "tensorflow.http",
                            "--reply_url",
                            "http://seldon-request-logger.seldon-logs",
                            "--event_type",
                            "io.seldon.serving.inference.outlier",
                            "--event_source",
                            "io.seldon.serving.angela-test-find me an outlier",
                            "OutlierDetector",
                            "--drift_batch_size",
                            "1000"
                        ],
                        "resources": {},
                        "securityContext": {
                            "runAsUser": 8888
                        }
                    }
                ]
            }
        }
    },
    "status": {}
}