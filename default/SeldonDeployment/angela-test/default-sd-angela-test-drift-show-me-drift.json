{
    "kind": "Service",
    "apiVersion": "serving.knative.dev/v1",
    "metadata": {
        "name": "default-sd-angela-test-drift-show-me-drift",
        "namespace": "default",
        "creationTimestamp": null,
        "labels": {
            "seldon.io/deployment-kind": "SeldonDeployment",
            "seldon.io/deployment-name": "angela-test",
            "seldon.io/deployment-namespace": "default",
            "seldon.io/detector-name": "show-me-drift",
            "seldon.io/detector-type": "drift"
        }
    },
    "spec": {
        "template": {
            "metadata": {
                "creationTimestamp": null,
                "labels": {
                    "fluentd": "true",
                    "seldon-deployment-id": "angela-test",
                    "seldon.io/drift": "true"
                },
                "annotations": {
                    "autoscaling.knative.dev/maxScale": "1",
                    "autoscaling.knative.dev/minScale": "1",
                    "prometheus.io/path": "/v1/metrics",
                    "prometheus.io/port": "8080",
                    "prometheus.io/scrape": "true"
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "drift-detector",
                        "image": "seldonio/alibi-detect-server:1.11.2",
                        "args": [
                            "--model_name",
                            "show-me-drift",
                            "--http_port",
                            "8080",
                            "--protocol",
                            "seldon.http",
                            "--storage_uri",
                            "gs://tom-seldon-examples/workshops/manufacturing/angela/defect-drift",
                            "--reply_url",
                            "http://seldon-request-logger.seldon-logs",
                            "--event_type",
                            "io.seldon.serving.inference.drift",
                            "--event_source",
                            "io.seldon.serving.angela-test-show-me-drift",
                            "DriftDetector",
                            "--drift_batch_size",
                            "2"
                        ],
                        "ports": [
                            {
                                "containerPort": 8080,
                                "protocol": "TCP"
                            }
                        ],
                        "env": [
                            {
                                "name": "SELDON_DEPLOYMENT_ID",
                                "value": "angela-test"
                            },
                            {
                                "name": "SELDON_DEPLOYMENT_NAMESPACE",
                                "value": "default"
                            }
                        ],
                        "resources": {},
                        "securityContext": {
                            "runAsUser": 8888
                        }
                    }
                ]
            }
        }
    },
    "status": {}
}